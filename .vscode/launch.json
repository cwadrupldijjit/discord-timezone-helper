{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch server",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--import",
                "./entry.mjs"
            ],
            "program": "index.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.(m|c|)js",
                "!**/node_modules/**"
            ],
            "console": "integratedTerminal"
        }
    ]
}
